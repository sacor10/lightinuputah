{
  "name": "lightinuputah",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "compression": "^1.7.4",
    "contentful": "^11.7.6",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "node server-heroku.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "node server.js",
    "serve-enhanced": "node server-enhanced.js",
    "dev": "npm run build && npm run serve",
    "dev-enhanced": "npm run build && npm run serve-enhanced",
    "prod": "NODE_ENV=production npm run build && npm run serve",
    "prod-enhanced": "NODE_ENV=production npm run build && npm run serve-enhanced",
    "deploy": "node deploy.js",
    "docker-build": "docker build -t lightin-up-utah .",
    "docker-run": "docker run -p 3000:3000 lightin-up-utah",
    "docker-compose-up": "docker-compose up -d",
    "docker-compose-down": "docker-compose down",
    "pm2-start": "pm2 start ecosystem.config.js",
    "pm2-stop": "pm2 stop ecosystem.config.js",
    "pm2-restart": "pm2 restart ecosystem.config.js",
    "pm2-logs": "pm2 logs",
    "pm2-monitor": "pm2 monit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
